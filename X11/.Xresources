! Font
Xft.antialias:  true
Xft.hinting:    true
Xft.hintstyle:  hintslight
Xft.lcdfilter:  lcddefault
Xft.dpi:        96
Xft.rgba:       none

! urxvt
URxvt.internalBorder:  10

URxvt.iso14755 : false

URxvt.font:        xft:Hack:pixelsize=11:antialias=true:hinting=true:autohint:true

URxvt.perl-ext-common: default,matcher,fullscreen,clipboard

URxvt.url-launcher:   /usr/bin/xdg-open
URxvt.matcher.button: 1

URxvt.keysym.F11: perl:fullscreen:switch
URxvt.keysym.C-C: perl:clipboard:copy
URxvt.keysym.C-V: perl:clipboard:paste
URxvt.keysym.C-A-V: perl:clipboard:paste_escaped

URxvt*scrollBar: false

! Colorscheme
*background: #212121
*foreground: #d9d9d9

*cursorColor: #20bbfc

*color0: #212121
*color8: #424242

*color1: #c30771
*color9: #fb007a

*color2: #10a778
*color10: #5fd7af

*color3: #a89c14
*color11: #f3e430

*color4: #008ec4
*color12: #20bbfc

*color5: #523c79
*color13: #6855de

*color6: #20a5ba
*color14: #4fb8cc

*color7: #d9d9d9
