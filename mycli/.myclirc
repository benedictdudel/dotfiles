[main]

# Completion
smart_completion = True
wider_completion_menu = False

# Prompt
prompt = '\t \u@\h:\d> '
prompt_continuation = ': '

# Interface
key_bindings = vi
table_format = psql
syntax_style = default

# Statement
timing = True
multi_line = True
destructive_warning = True

# Log
log_level = INFO
log_file = ~/.mycli.log
